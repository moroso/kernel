use file::*;
use mallot::*;
use syslib::*;
use prelude::rt_abort;

const NUM_THREADS: u32 = 10;
const MB: i32 = 1; // NOTE this is not guaranteed -- we are assuming 

fn main() -> i32 {
    printf!("initing file server\n");
    if (mallot_init() < 0) {
        printf!("err initing file server\n");
        abscond(-1);
    }

    let ret = file_server_init(NUM_THREADS, MB);
    assert!(ret < 0);
    abscond(ret);
    0
}
