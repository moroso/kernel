use file::*;
use name::*;
use syslib::*;
use ipclib::*;
use mallot::*;


fn main() -> i32 {

    mallot_init();
    let mb = mailbox_new();
    let name_mb: i32 = -1;
    if ((name_mb = name_server_init(5, mb)) < 0) {
        printf!("error\n");
        abscond(-1);
    }
    mailbox_move(name_mb as u32, NAME_SERVER);
    perform("file_server_test", null);

    while(true) {}

    0
}
