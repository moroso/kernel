extern "bare" fn __smul_32_32(a: i32, b: i32, top_out: *i32, bot_out: *u32) {
    asm!(
            "{ r0 <- r0 *s r1; }"
            "{ r1 <- OVF; }"
            "{ *l(r2) <- r1; *l(r3) <- r0; }"
            "{ b r31 + 1; }"
            );
}

extern "bare" fn __sdiv_64_32(ntop: i32, nbot: u32, d: i32) -> i32 {
    asm!(
            "{ ovf <- r0; }"
            "{ r0 <- r1 /sw r2; }"
            "{ b r31 + 1; }"
            );
}
